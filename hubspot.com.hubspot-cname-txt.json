{
  "providerId": "hubspot.com",
  "providerName": "HubSpot",
  "serviceId": "hubspot-cname-txt",
  "serviceName": "HubSpot Site",
  "version": 1,
  "logoUrl": "https://domainconnect.org/wp-content/uploads/2017/02/HubSpot-768x223.png",
  "description": "Template for for domain verification workflow",
  "variableDescription": "txtValue - txt data value, cnameValue - cname record pointsTo value",
  "hostRequired": true,
  "records": [
    {
      "groupId": "txt",
      "type": "TXT",
      "host": "@",
      "data": "%txtValue%",
      "ttl": 3600
    },
    {
      "groupId": "cname",
      "type": "CNAME",
      "host": "@",
      "pointsTo": "%cnameValue%",
      "ttl": 3600
    }
  ]
}
