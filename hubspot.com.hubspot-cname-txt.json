{
  "providerId": "hubspot.com",
  "providerName": "HubSpot",
  "serviceId": "hubspot-cname-txt",
  "serviceName": "HubSpot Site",
  "version": 1,
  "logoUrl": "https://domainconnect.org/wp-content/uploads/2017/02/HubSpot-768x223.png",
  "description": "Enables a domain to work with HubSpot",
  "variableDescription": "data - txt data value, cname - pointTo value",
  "hostRequired": true,
  "records": [
    {
      "groupId": "txt",
      "type": "TXT",
      "host": "@",
      "data": "%data%",
      "ttl": 3600
    },
    {
      "groupId": "cname",
      "type": "CNAME",
      "host": "@",
      "pointsTo": "%cname%",
      "ttl": 3600
    }
  ]
}
