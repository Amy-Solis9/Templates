{
  "providerId": "hubspot.com",
  "providerName": "HubSpot",
  "serviceId": "hubspot-cname",
  "serviceName": "HubSpot Site",
  "version": 1,
  "logoUrl": "https://domainconnect.org/wp-content/uploads/2017/02/HubSpot-768x223.png",
  "description": "Enables a sub-domain to work with HubSpot",
  "variableDescription": "cnameValue - cname record pointsTo value, cNameHostValue - cname host value",
  "hostRequired": true,
  "records": [
    {
      "groupId": "cname",
      "type": "CNAME",
      "host": "%cNameHostValue%",
      "pointsTo": "%cnameValue%",
      "ttl": 3600
    }
  ]
}
