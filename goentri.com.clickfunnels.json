{
    "providerId": "goentri.com",
    "providerName": "Entri",
    "serviceId": "clickfunnels",
    "serviceName": "ClickFunnels",
    "version": 1,
    "logoUrl": "https://cdn.goentri.com/logo.svg",
    "description": "Allows user to easily set up domain using Entri",
    "syncPubKeyDomain": "goentri.com",
    "syncRedirectDomain": "api.goentri.com, goentri.com, entri.com",
    "variableDescription": "verificationValue is the value that is used to verify domain ownership,clusterValue is the cluster assigned to the domain, host is the domain host,domainId is the id of domain, keyValue is the generated key value",
    "sharedProviderName": true,
    "records": [
      {
        "type": "TXT",
        "host": "@",
        "pointsTo": "clickfunnels-domain-verification=%verificationValue%",
        "ttl": 3600
      },
      {
        "type": "CNAME",
        "host": "%host%",
        "pointsTo": "%clusterValue",
        "ttl": 3600
      },
      {
        "type": "TXT",
        "host": "%websiteId%._domainkey.%host%",
        "pointsTo": "%keyValue%",
        "ttl": 3600
      }
    ]
  }