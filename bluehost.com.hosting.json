{
  "providerId": "bluehost.com",
  "providerName": "Bluehost",
  "serviceId": "hosting",
  "serviceName": "hosting",
  "version": 1,
  "syncPubKeyDomain": "domainconnect.bluehost.com",
  "syncRedirectDomain": "my.bluehost.com",
  "logoUrl": "https://bluehost-cdn.com/media/app/bluehost/live/static/media/src/common/assets/images/brand/bh/logo.svg",
  "description": "Enables a domain to work with Bluehost hosting",
  "variableDescription": "ip is the IP address of the server, and box is the hostname of the server.",
  "records": [
    {
      "pointsTo": "%ip%",
      "type": "A",
      "host": "@",
      "ttl": 14400
    },
    {
      "pointsTo": "%ip%",
      "type": "A",
      "host": "whm",
      "ttl": 14400
    },
    {
      "pointsTo": "%ip%",
      "type": "A",
      "host": "mail",
      "ttl": 14400
    },
    {
      "pointsTo": "%ip%",
      "type": "A",
      "host": "cpcalendars",
      "ttl": 14400
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "www",
      "ttl": 14400
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "ftp",
      "ttl": 14400
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "cpanel",
      "ttl": 14400
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "webmail",
      "ttl": 14400
    },
    {
      "pointsTo": "mail.%domain%",
      "type": "CNAME",
      "host": "imap",
      "ttl": 14400
    },
    {
      "pointsTo": "mail.%domain%",
      "type": "MX",
      "host": "@",
      "ttl": 14400,
      "priority": 0
    },
    {
      "data": "v=spf1 a mx include:websitewelcome.com ~all",
      "type": "TXT",
      "host": "@",
      "ttl": 14400,
      "txtConflictMatchingMode": "Prefix",
      "txtConflictMatchingPrefix": "v=spf1"
    },
    {
      "data": "path=/",
      "type": "TXT",
      "host": "_caldav._tcp",
      "ttl": 14400,
      "txtConflictMatchingMode": "None"
    },
    {
      "data": "path=/",
      "type": "TXT",
      "host": "_caldavs._tcp",
      "ttl": 14400,
      "txtConflictMatchingMode": "None"
    },
    {
      "data": "path=/",
      "type": "TXT",
      "host": "_carddav._tcp",
      "ttl": 14400,
      "txtConflictMatchingMode": "None"
    },
    {
      "data": "path=/",
      "type": "TXT",
      "host": "_carddavs._tcp",
      "ttl": 14400,
      "txtConflictMatchingMode": "None"
    },
    {
      "target": "emaildiscovery.cpanel.net",
      "type": "SRV",
      "service": "_autodiscover",
      "protocol": "_tcp",
      "name": "",
      "ttl": 14400,
      "priority": 0,
      "weight": 0,
      "port": 443
    },
    {
      "target": "%box%",
      "type": "SRV",
      "service": "_caldav",
      "protocol": "_tcp",
      "name": "",
      "ttl": 14400,
      "priority": 0,
      "weight": 0,
      "port": 2079
    },
    {
      "target": "%box%",
      "type": "SRV",
      "service": "_carddav",
      "protocol": "_tcp",
      "name": "",
      "ttl": 14400,
      "priority": 0,
      "weight": 0,
      "port": 2079
    },
    {
      "target": "%box%",
      "type": "SRV",
      "service": "_caldavs",
      "protocol": "_tcp",
      "name": "",
      "ttl": 14400,
      "priority": 0,
      "weight": 0,
      "port": 2080
    },
    {
      "target": "%box%",
      "type": "SRV",
      "service": "_carddavs",
      "protocol": "_tcp",
      "name": "",
      "ttl": 14400,
      "priority": 0,
      "weight": 0,
      "port": 2080
    }
  ]
}

