{
  "providerId":"websitetoolbox.com",
  "providerName":"Website Toolbox",
  "serviceId":"apexforum",
  "serviceName":"Website Toolbox Forum",
  "version": 1,
  "syncPubKeyDomain" : "websitetoolbox.com",
  "logoUrl":"https://cdn.websitetoolbox.com/images/logos/wt_logo_transparent.png",
  "description":"Enables the root of a domain to work with Website Toolbox.",
  "variableDescription":"",
  "records":[
    {
      "type":"REDIR301",
      "host": "@",
      "target":"https://www.%fqdn%"
    },
    {
      "type":"CNAME",
      "host":"www",
      "pointsTo":"custom.websitetoolbox.com",
      "ttl":3600
    },
    {
      "type": "TXT",
      "host": "@",
      "data": "v=spf1 a mx a:mailers.websitetoolbox.com ~all",
      "ttl": 3600
    },
    {
      "type": "CNAME",
      "host": "websitetoolbox._domainkey",
      "pointsTo": "websitetoolbox._domainkey.websitetoolbox.com",
      "ttl": 3600
    }
  ]
}
